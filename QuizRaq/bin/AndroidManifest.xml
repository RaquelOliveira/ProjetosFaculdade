<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.quiz.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.quiz.SegundaActivity"
            android:label="@string/title_activity_segunda" >
        </activity>
        <activity
            android:name="com.example.quiz.TerceiraActivity"
            android:label="@string/title_activity_terceira" >
        </activity>
        <activity
            android:name="com.example.quiz.QuartaActivity"
            android:label="@string/title_activity_quarta" >
        </activity>
        <activity
            android:name="com.example.quiz.ResultadoActivity"
            android:label="@string/title_activity_resultado" >
        </activity>
    </application>

</manifest>
